<%=
  secondary_navigation_items = [
    [
      label: "Overview",
      path: &Routes.team_path(&1, :index),
      path_match_type: :exact,
      verify: &has?(&1, "teams:list")
    ],
    [
      label: "Event Logs",
      path: &Routes.team_path(&1, :index_event_log_list),
      path_match_type: :starts_with,
      verify: &has?(&1, "teams:list")
    ]
  ]

  render_secondary_navigation(
    @conn,
    current_user(@conn),
    secondary_navigation_items
  )
%>
